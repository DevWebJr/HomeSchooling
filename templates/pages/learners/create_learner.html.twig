{% extends "base.html.twig" %}


{% block title %}Ajouter un Apprenant{% endblock %}

{% block body %}
<div class="block">
    <form>
        {{ form_start(form) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
        {{ form_row(form.gender) }}
        <button type="submit">Valider</button>
        {{ form_end(form) }}
    </form>
</div>
{% endblock %}